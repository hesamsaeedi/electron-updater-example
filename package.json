{
  "name": "electron-updater-example",
  "version": "0.6.4",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Hesam Saeedi",
  "scripts": {
    "publish": "build -p always"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^20.28.4"
  },
  "dependencies": {
    "electron-log": "^1.3.0",
    "electron-updater": "^3.0.3"
  },
  "build": {
    "appId": "com.github.hesamsaeedi.electronupdaterexample",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "Linux": {
      "target": [
        "AppImage"
      ]}
  }
}
